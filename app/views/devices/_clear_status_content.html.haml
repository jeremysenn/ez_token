%p
  =# device.caution_status_description
  =# device['caution_status']
  =# StatusDesc.find_by_status(device['caution_status'])['short_desc'] unless device['caution_status'].blank?
= link_to "Cancel", '#', class: 'btn btn-default', onclick: "$(this).closest('div.popover').prev().popover('hide');", value: "close"
=# link_to "Clear Status", clear_status_device_path(device.id), class: 'btn btn-primary'