%table.table.table-striped.table-hover.table-condensed
  %thead
    %tr
      %th= "Date"
      %th= "Status"

  %tbody
    - @wsdl_dev_statuses.each do |dev_status|
      %tr
        %td= dev_status['date_time'].to_datetime.strftime("%-m/%-d/%y %H:%M:%S")
        %td= dev_status['status']


-#
  %table.table.table-striped.table-hover
    %thead
      %tr
        %th= "Date"
        %th= "Status"
        %th= "Raw Status"

    %tbody
      - @dev_statuses.each do |dev_status|
        %tr
          %td= dev_status.date_time.strftime("%-m/%-d/%y %H:%M:%S")
          %td= dev_status.status_description
          %td= dev_status.raw_status

  = paginate @dev_statuses, :param_name => "dev_statuses_page"
            