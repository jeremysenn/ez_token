-# .wrapper
- if user_signed_in?
  =# render 'admin_user_sidebar'
  - if current_user.admin? or current_user.collaborator?
    - if browser.device.mobile?
      = render 'mobile_admin_user_start' 
    - else
      = render 'admin_user_start'
  - elsif current_user.basic?
    = render 'basic_user_start'
- else
  = render 'no_user_start'
