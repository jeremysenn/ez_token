.wrapper
  - if user_signed_in?
    =# render 'admin_user_sidebar'
    - if current_user.admin? or current_user.basic?
      - if browser.device.mobile?
        = render 'mobile_admin_user_start' 
      - else
        = render 'admin_user_start'
    - elsif current_user.payee?
      = render 'customer_user_start'
    - elsif current_user.consumer?
      = render 'consumer_user_start'
    - elsif current_user.vendor?
      = render 'vendor_user_start'
  - else
    = render 'no_user_start'
